const endereço = "Avenida 1";

let preçoLeite = 9.5;
let preçoQueijo = 30;

console.log("Preço do Leite", preçoLeite);
console.log("Preço do Queijo", preçoQueijo);

preçoLeite = 8;
//o codigo é exibido em ordem de linha por linha, até esse momento 
//o preço do leite continua sendo exibido como 9.5, pois segue-se a ordem
console.log("Preço do Leite", preçoLeite);
console.log("Preço do Queijo", preçoQueijo);
//agora sim exibe o novo valor
//a constante tem que ter um valor inicial declarado

let resposta = 0; //checar os numeros que são multiplos de 17

let numero = 1;

while (numero <= 1000) // quero repetir enquanto até que esse numero chegue a 1000, quanto ele for menor que  1000
{
  if(numero % 17 === 0)
  {
    resposta += 1;
  }
  numero += 1;
}

console.log ("Resposta é", resposta);
